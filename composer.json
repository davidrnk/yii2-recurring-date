{
    "name": "davidrnk/yii2-recurring-date",
    "description": "A Yii2 extension that allows users to define and handle recurring date intervals for renewals, expirations and scheduling.",
    "keywords": ["yii2", "recurring", "date", "recurrence"],
    "type": "yii2-extension",
    "require": {
        "php": ">=7.4",
        "yiisoft/yii2": "^2.0.49",
        "yiisoft/yii2-bootstrap5": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.0",
        "yiisoft/yii2-codeception": "2.0.6"
    },
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "davidrnk\\RecurringDate\\": "src/"
        }
    },
    "authors": [
        {
            "name": "David Ramirez C.",
            "email": "ldrcrnk.dev@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}